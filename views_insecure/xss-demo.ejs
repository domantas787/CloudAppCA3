<%- include('layout', { body: '' }) %>
<h2>DOM XSS Demo</h2>
<div id="output"></div>

<script>
  const params = new URLSearchParams(location.search);
  const value = params.get('msg') || 'No message';
  // DOM-based XSS: using innerHTML
  document.getElementById('output').innerHTML = value;
</script>
